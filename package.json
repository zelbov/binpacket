{
  "name": "binpacket",
  "version": "0.0.4",
  "description": "Binary data builder/parser with decorators support for structures definition",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc",
    "watch": "npx tsc --watch",
    "test": "./node_modules/.bin/mocha -r ts-node/register ./test/unit/*.test.ts",
    "bench": "./node_modules/.bin/mocha -r ts-node/register ./test/benchmark/*.test.ts",
    "ci": "circleci config process .circleci/config.yml > temp/processed.yml && circleci local execute -c temp/processed.yml",
    "lint": "./node_modules/.bin/eslint --ext ts",
    "postinstall": "cat package-lock.json | md5sum > package.checksum"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zelbov/binpacket.git"
  },
  "author": "https://github.com/zelbov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zelbov/binpacket/issues"
  },
  "homepage": "https://github.com/zelbov/binpacket#readme",
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.42",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "chai": "^4.3.6",
    "mocha": "^9.2.1",
    "ts-node": "^10.5.0"
  }
}
