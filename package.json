{
  "name": "binpacket",
  "version": "0.0.4",
  "type": "module",
  "description": "Binary data builder/parser with decorators support for structures definition",
  "main": "dist/src/index.js",
  "scripts": {
    "build": "npx tsc",
    "watch": "npx tsc --watch",
    "test": "./node_modules/.bin/mocha --require esm --experimental-specifier-resolution=node --reporter mocha-multi-reporters --reporter-options configFile=test/config.json,cmrOutput=mocha-junit-reporter+mochaFile+test ./dist/test/unit/*.test.js",
    "bench": "./node_modules/.bin/mocha --require esm --experimental-specifier-resolution=node --reporter mocha-multi-reporters --reporter-options configFile=test/config.json,cmrOutput=mocha-junit-reporter+mochaFile+bench ./dist/test/benchmark/*.test.js",
    "ci": "circleci config process .circleci/config.yml > temp/processed.yml && circleci local execute -v $(pwd)/temp/project:/root/project -c temp/processed.yml",
    "lint": "./node_modules/.bin/eslint --ext ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zelbov/binpacket.git"
  },
  "author": "https://github.com/zelbov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zelbov/binpacket/issues"
  },
  "homepage": "https://github.com/zelbov/binpacket#readme",
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.42",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "chai": "^4.3.6",
    "esm": "^3.2.25",
    "mocha": "^9.2.1",
    "mocha-junit-reporter": "^2.0.2",
    "mocha-multi-reporters": "^1.5.1",
    "ts-node": "^10.5.0",
    "typescript": "^4.7.4"
  }
}
